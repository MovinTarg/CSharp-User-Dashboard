@{
    ViewData["Title"] = "DashBoard";
}
<h1>All Users</h1>
@{
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Email</th>
                <th>Created At</th>
                <th>User Level</th>
            </tr>
        </thead>
        <tbody>
        @foreach(var user in ViewBag.allUsers)
        {
            <tr>
                <td>@user.UserId</td>
                <td><a href="users/show/@user.UserId">@user.FirstName @user.LastName</a></td>
                <td>@user.Email</td>
                <td>@user.CreatedAt</td>
                <td>@user.Level</td>
            </tr>
        }
        </tbody>
    </table>
}